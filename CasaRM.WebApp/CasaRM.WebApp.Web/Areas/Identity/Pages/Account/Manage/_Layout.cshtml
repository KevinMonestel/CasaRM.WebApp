@{
    Layout = "../../../../../Views/Shared/_Layout.cshtml";
}

<h1 class="h1 mb-3"><strong>Account</strong> Manage</h1>

<div class="card">
    <div class="card-header">
        <h3>Change your account settings</h3>
    </div>

    <div class="card-body">
        <div class="row">
            <div class="col-md-3">
                <partial name="_ManageNav" />
            </div>
            <div class="col-md-9">
                @RenderBody()
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
